@{
    ViewData["Title"] = "Home Page";
}
@model Assignment10.Models.ViewModels.IndexViewModel

    

<div>
    @foreach (var x in Model.Bowlers)
    {
        <p>@x.BowlerFirstName @x.BowlerMiddleInit @x.BowlerLastName</p>
        <p>@x.BowlerAddress @x.BowlerCity, @x.BowlerState, @x.BowlerZip</p>
        <p>@x.BowlerPhoneNumber</p>
        <br />
    }
</div>

<!--This is where there thee page numbers should be included-->
<div page-info="@Model.PageNumberingInfo"
     page-url-teamtype ="@Model.TeamCategory">
</div>